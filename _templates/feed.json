{
    "version": "https://jsonfeed.org/version/1.1",
    "title": "Gavin Wiggins",
    "home_page_url": "https://gavinw.me",
    "feed_url": "https://gavinw.me/feed.json",
    "items": [
    {% for feed in feeds %}
        {
            "id": "{{ feed.url }}",
            "url": "{{ feed.url }}",
            "title": "{{ feed.title }}",
            "date_published": "{{ feed.date }}",
            "content_html": {{ feed.html }}
        }{% if not loop.last %},{% endif +%}
    {% endfor %}
    ]
}